<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" id="WebApp_ID" version="5.0">
  <display-name>01_TestMavenEclipse</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>login</display-name>
    <servlet-name>login</servlet-name>
    <servlet-class>com.durante.login</servlet-class>
    <init-param>
    	<param-name>db_url</param-name>
    	<param-value>localhost:3306/durante_01_database</param-value>
    </init-param>
    <init-param>
    	<param-name>db_usr</param-name>
    	<param-value>admin</param-value>
    </init-param>
    <init-param>
    	<param-name>db_psw</param-name>
    	<param-value>admin</param-value>
    </init-param>
  </servlet>
  <servlet>
    <description></description>
    <display-name>preferenza</display-name>
    <servlet-name>preferenza</servlet-name>
    <servlet-class>com.durante.preferenza</servlet-class>
    <init-param>
    	<param-name>db_url</param-name>
    	<param-value>localhost:3306/durante_01_database</param-value>
    </init-param>
    <init-param>
    	<param-name>db_usr</param-name>
    	<param-value>admin</param-value>
    </init-param>
    <init-param>
    	<param-name>db_psw</param-name>
    	<param-value>admin</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>preferenza</servlet-name>
    <url-pattern>/preferenza</url-pattern>
  </servlet-mapping>
</web-app>